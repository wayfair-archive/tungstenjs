{
  "name": "tungstenjs",
  "version": "0.11.2",
  "description": "A modular framework for creating web UIs with high-performance rendering on both server and client.",
  "author": "Matt DeGennaro <mdegennaro@wayfair.com>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/wayfair/tungstenjs.git"
  },
  "bugs": "https://github.com/wayfair/tungstenjs/issues",
  "main": "./dist/tungsten.backbone.js",
  "scripts": {
    "clean": "rm dist/*.js",
    "jasmine": "node test/jasmine.js",
    "pretest": "npm run eslint",
    "test": "npm run tests",
    "tests": "node ./test/build.js && npm run jasmine",
    "build-plugins": "webpack --config ./webpack.plugins.js",
    "build-bb-dist-dev": "webpack --config ./webpack.standalone.js --adaptor=backbone --dev=true",
    "build-bb-dist": "webpack --config ./webpack.standalone.js --adaptor=backbone",
    "build-ampersand-dist-dev": "webpack --config ./webpack.standalone.js --adaptor=ampersand --dev=true",
    "build-ampersand-dist": "webpack --config ./webpack.standalone.js --adaptor=ampersand",
    "eslint": "node node_modules/eslint/bin/eslint src adaptors test precompile",
    "cover": "node ./test/build.js --coverage && npm run jasmine",
    "prepublish": "npm run build-plugins && npm run build-bb-dist && npm run build-ampersand-dist && npm run build-bb-dist-dev && npm run build-ampersand-dist-dev"
  },
  "dependencies": {
    "ampersand-collection": "^1.4.5",
    "ampersand-model": "^5.0.3",
    "ampersand-view": "^7.3.0",
    "babel-plugin-transform-es2015-arrow-functions": "^6.1.18",
    "babel-plugin-transform-es2015-parameters": "^6.3.13",
    "babel-plugin-transform-es2015-shorthand-properties": "^6.3.13",
    "backbone": "^1.1.2",
    "backbone.nativeajax": "^0.4.3",
    "data-set": "^4.0.0",
    "entities": "^1.1.1",
    "extend": "^3.0.0",
    "form-serialize": "^0.6.0",
    "global": "^4.3.0",
    "htmlparser2": "^3.8.3",
    "jsdom": "^6.5.0",
    "json-loader": "^0.5.2",
    "punch": "^0.5.46",
    "purecss": "^0.6.0",
    "underscore": "^1.7.0",
    "virtual-dom": "^2.0.1",
    "webpack-strip-block": "0.0.1",
    "x-is-array": "^0.1.0"
  },
  "devDependencies": {
    "babel-core": "6.3.26",
    "babel-loader": "6.2.1",
    "babel-plugin-transform-es2015-block-scoping": "^6.1.18",
    "babel-plugin-transform-es2015-constants": "^6.1.4",
    "chai": "^2.2.0",
    "coveralls": "^2.11.4",
    "eslint": "2.2.0",
    "express": "^4.13.3",
    "glob": "^4.3.5",
    "hogan-express": "^0.5.2",
    "isparta-loader": "^0.2.0",
    "istanbul": "^0.3.19",
    "jasmine": "^2.3.1",
    "loader-utils": "^0.2.11",
    "mkdirp": "~0.5.0",
    "opener": "^1.4.1",
    "webpack": "^1.5.1",
    "webpack-dev-middleware": "^1.2.0"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "directories": {
    "example": "./examples",
    "lib": "./src"
  }
}
