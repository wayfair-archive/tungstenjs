{
  "name": "tungstenjs",
  "version": "0.5.4",
  "description": "A modular framework for creating web UIs with high-performance rendering on both server and client.",
  "author": "Matt DeGennaro <mdegennaro@wayfair.com>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/wayfair/tungstenjs.git"
  },
  "bugs": "https://github.com/wayfair/tungstenjs/issues",
  "main": "tungsten.js",
  "scripts": {
    "jasmine": "node node_modules/jasmine/bin/jasmine JASMINE_CONFIG_PATH=test/jasmine.json",
    "test-debug": "npm run build-test-debug && npm run jasmine",
    "pretest": "npm run eslint && npm run jscs",
    "test": "npm run build-test && npm run jasmine",
    "build-test": "webpack  ./src/tungsten ./dist/tungsten.test.js --config webpack.test.config.js",
    "build-test-debug": "webpack  ./src/tungsten ./dist/tungsten.test.js --config webpack.test.config.js --dev",
    "build-bb-dist-dev": "webpack  ./src/tungsten ./dist/tungsten.js -d && webpack  ./adaptors/backbone ./dist/tungsten.backbone.js -d",
    "build-bb-dist": "webpack  ./src/tungsten ./dist/tungsten.min.js -p && webpack  ./adaptors/backbone ./dist/tungsten.backbone.min.js -p",
    "build-ampersand-dist-dev": "webpack  ./src/tungsten ./dist/tungsten.js -d && webpack  ./adaptors/ampersand ./dist/tungsten.ampersand.js -d",
    "build-ampersand-dist": "webpack  ./src/tungsten ./dist/tungsten.min.js -p && webpack  ./adaptors/ampersand ./dist/tungsten.ampersand.min.js -p",
    "jscs": "node node_modules/jscs/bin/jscs src adaptors test precompile",
    "eslint": "node node_modules/eslint/bin/eslint src adaptors test precompile",
    "jsbeautify": "find ./src ./adaptors ./test ./precompile -name '*.js' | xargs ./node_modules/.bin/js-beautify -r",
    "prepublish": "[ -r ~/.global.npmignore ] && cat ~/.global.npmignore >> .npmignore",
    "publish": "git checkout .npmignore"
  },
  "dependencies": {
    "ampersand-collection": "^1.4.5",
    "ampersand-model": "^5.0.3",
    "ampersand-view": "^7.3.0",
    "backbone": "^1.1.2",
    "backbone.native": "~1.0.0",
    "data-set": "^4.0.0",
    "html-tokenizer": "^1.4.4",
    "json-loader": "^0.5.2",
    "ractive": "^0.6.1",
    "underscore": "^1.7.0",
    "virtual-dom": "^2.0.1",
    "webpack-strip-block": "0.0.1",
    "x-is-array": "^0.1.0"
  },
  "devDependencies": {
    "chai": "^2.2.0",
    "eslint": "^1.2.1",
    "express": "^4.13.3",
    "glob": "^4.3.5",
    "hogan-express": "^0.5.2",
    "jasmine": "^2.3.1",
    "js-beautify": "^1.5.10",
    "jscs": "^2.1.0",
    "jsdom-nogyp": "^0.8.3",
    "mkdirp": "~0.5.0",
    "opener": "^1.4.1",
    "webpack": "^1.5.1",
    "webpack-dev-middleware": "^1.2.0"
  },
  "directories": {
    "example": "./examples",
    "lib": "./src"
  }
}
